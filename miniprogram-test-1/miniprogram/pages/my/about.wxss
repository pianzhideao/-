/* list列表样式 */
.list{
  margin: 10px 0;
  /* background: pink; */
  line-height: 88px;
  font-size: 17pt;
  color: #000;
}
.list_1{
	position: relative;
  background: #FEFEFE;
  margin: 10px 0;

  
  /* border-bottom: 0.5px solid #eee; */
}
.list_2{
	position: relative;
	vertical-align: middle;
	  /* display: block; */
  border-bottom: 0.5px solid #eee;
  height:50px;
  line-height: 50px;

}

.list_3{ 
	position: absolute;
	left: 20px;
  display: block;
	font-size: 14px;
  border-bottom: 0.5px solid #eee;
  height:50px;
  line-height: 50px;
}
